{
  "name": "engineering-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx open-next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build && wrangler pages deploy .open-next/output",
    "db:init": "npx wrangler d1 execute engineering-studio-db --file=./db/schema.sql --local",
    "db:deploy": "npx wrangler d1 execute engineering-studio-db --file=./db/schema.sql",
    "r2:init": "npx wrangler r2 bucket create engineering-studio-r2",
    "vectorize:init": "npx wrangler vectorize create engineering-studio-vectors --dimensions=768 --metric=cosine"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
