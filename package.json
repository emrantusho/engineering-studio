    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@tanstack/react-virtual": "^3.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "hono": "^4.2.3",
    "lucide-react": "^0.368.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
